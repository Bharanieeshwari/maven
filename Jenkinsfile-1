pipeline {
    agent {
    node {
        label 'ecsagent'
    }
}

    stages {

        stage('Build Java-Web-Application') {
            steps {
                sh 'docker build -t dishoneprabu/java-web-app:latest .'
            }
        }
    }

    post {
        always {
            echo 'Build Completed'
        }
    }
}